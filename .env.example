# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# App URL
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Development
NODE_ENV="development"

# Stripe
STRIPE_SECRET_KEY=sk_test_your_secret_key
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret
STRIPE_PRICE_ID_MONTHLY=price_monthly_id
STRIPE_PRICE_ID_YEARLY=price_yearly_id
# Optional: Set a test clock ID to test subscription renewals/cancellations (leave empty for real-time)
STRIPE_TEST_CLOCK_ID=

# Use this to force nixpacks to use node 20 instead of 18, which is its default version. This is needed when deploying on coolify.
NIXPACKS_NODE_VERSION=20

OPENAI_API_KEY=your-api-key
